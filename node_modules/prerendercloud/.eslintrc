{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-use-before-define": 2,
    "consistent-return": 2,
    "prefer-const": 2,
    "class-methods-use-this": 1,
    "block-scoped-var": 2,
    "vars-on-top": 2,
    "no-shadow": 2,
    "no-param-reassign": 2
  },
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
